<?xml version="1.0" encoding="UTF-8"?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui" 
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:local="clr-namespace:SolverApp.Views"
       x:Class="SolverApp.AppShell">
    <TabBar>
        <Tab Title="Solver"
            Icon="IconOptions.png">
            <ShellContent x:Name="ShellSolverPage" Route="SolverPage"/>
        </Tab>
        <Tab Title="New Map"
            Icon="IconPlus.png">
            <ShellContent x:Name="ShellNewMapPage" Route="NewMapPage"/>
        </Tab> 
        <Tab Title="Photo"
            Icon="IconPhoto.png">
            <ShellContent x:Name="ShellPhotoHelperPage" Route="PhotoHelperPage"/>
        </Tab>
    </TabBar>
</Shell>
